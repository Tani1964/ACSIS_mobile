{
  "expo": {
    "name": "ACSIS",
    "slug": "acsis",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/adaptive-icon.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/images/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "buundleIdentifier": "com.acsis.googleauthexporeactnative",
      "supportsTablet": true,
      "infoPlist": {
        "CFBundleURLTypes": [
          {
            "CFBundleURLSchemes": [
              "com.googleusercontent.apps.1077796380960-mv6j98d90g6gjfpjrt41folnoqrk2ppt"
            ]
          }
        ]
      },
      "bundleIdentifier": "com.tani1964.ACSIS",
      "config": {
        "googleMapsApiKey": "AIzaSyCWDZCZ1ewtuTm5hl2euGj0mrMn-F0DJIw"
      }
    },
    "android": {
      "package": "acsis.mobile",
      "config":{
        "googleMaps": {
        "apiKey": "AIzaSyCWDZCZ1ewtuTm5hl2euGj0mrMn-F0DJIw"
      }
      },
      "versionCode": 1,
      "packages": "com.acsis.googleauthexporeactnative",
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/icon.png",
        "backgroundColor": "#ffffff"
      },
      "permissions": [
        "android.permission.ACCESS_COARSE_LOCATION",
        "android.permission.ACCESS_FINE_LOCATION"
      ]
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      "expo-secure-store",
      [
        "@react-native-google-signin/google-signin",
        {
          "iosUrlScheme": "com.googleusercontent.apps.1077796380960-mv6j98d90g6gjfpjrt41folnoqrk2ppt"
        }
      ],
      [
        "expo-location",
        {
          "locationAlwaysAndWhenInUsePermission": "Allow ACSIS to use your location."
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "27c1f304-6246-4f10-a9d3-9f79425eed02"
      }
    },
    "owner": "tani1964"
  }
}
